#include <iostream>
using namespace std;

int main (){
	string password;
	
	cout << "==========LOGIN==========" << endl;
	cout << "Masukan pasword :" ;
	cin >> password ;
	
	if (password == "terserah"){
		cout << "SELAMAT DATANG DI WELCOME" << endl;
	} else {
		cout << "SALAH PASWORDMU, COBA LAGI OM !!" << endl;
		
	}
	cout << "TERIMAKASIH SUDAH MENGGUNAKAN PROGRAM INI" << endl;
	
	return 0;
}
